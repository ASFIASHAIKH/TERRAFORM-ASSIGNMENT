# Create a VPC
AWS-VPC = "MY-VPC"

# Create Public Subnets
AWS-PUB-SUB-01 = "PUB-SUB-1a"
AWS-PUB-SUB-02 = "PUB-SUB-1b"

# 2. Create 2 Private Subnets
AWS-PRI-SUB-01 = "PRI-SUB-1a"
AWS-PRI-SUB-02 = "PRI-SUB-1b"

# 3. Create IGW (Internet Gateway) & Attach to the VPC
AWS-IGW = "MY-INTERNET-GW"

# 4. Create Public and Private Route Table
AWS-PUB-ROUTE = "Public-Route"
AWS-PRI-ROUTE = "Private-Route"
#EC2 INSTANCE
AWS-INST-IMG-ID = "ami-00952f27cf14db9cd"
AWS-INST-TYPE   = "t2.micro"
AWS-INST-NAME   = "HELLO-ASFIA"
ASSOC-PUB-IP    = true

#SSH KEY-PAIR 
AWS-KEY-NAME = "TEAM-KEY"
AWS-PUB-KEY  = "ssh-rsa "

#SSH SECURITY GROUP
AWS-SG-NAME    = "TEAM-SG"
AWS-SG-DESC    = "TEAM-SG"
AWS-SG-PORT    = [22, 80, 443]
AWS-SG-PROTO   = "tcp"
AWS-CIDR-BLOCK = ["0.0.0.0/0"]
